{
	"manifest_version": 2,
	"name": "AntiNÃ“NIO",
	"version": "0.1",
	"browser_action": {
		"icons": {
			"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png"
		}
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
